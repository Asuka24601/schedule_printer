/* custom-tab-bar/index.wxss */
.hide {
  pointer-events: none;
  animation:tohide 0.25s linear;
  /* opacity: 0; */
  transform: scale(0);
}

@keyframes tohide {
  from{
    opacity: 0.6;
    transform: scale(100%);
  }
  to{
    opacity: 0;
    transform: scale(0);
  }
}

.rhide {
  animation:rtohide 0.25s linear;
  /* opacity: 0.6; */
  transform: scale(100%);
}

@keyframes rtohide {
  from{
    opacity: 0;
    transform: scale(0);
  }
  to{
    opacity: 0.6;
    transform: scale(100%);
  }
}

.navbar {
  position: absolute;
  display:flex;
  flex-direction: column;
  z-index: 999;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  background-clip: content-box;
  /* padding: 0 15vw 0 15vw; */
  height: 50vw;
  width: 50vw;
  background-color: #6fc8ff95;
  /* margin-bottom: 60px; */

  right:-50px;
  bottom: -50px;
}

.icon_bar {
  width: 40vw;
  /* height: 24px; */
  position: absolute;
}

.icon_bar.bar1 {
  transform: rotateZ(calc(20deg));
}
.icon_bar.bar2 {
  transform: rotateZ(calc(65deg));
}
.icon_bar.bar3 {
  transform: rotateZ(calc(110deg));
}
.icon_bar.bar4 {
  transform: rotateZ(calc(-25deg));
}

