/* pages/user/user.wxss */
.belowInterface {
  z-index: -1000;
  position: fixed;
  right: 0;
  top: 0;
  width: calc(100vw - 72.51vw);
  height: 100vh;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  transform:none
}

.belowInterface .opentions {
  position: relative;
  margin-top: 50%;
  font-weight: bold;
  text-shadow: .1px .1px 0.75px black;

  border: 1.5px solid black;
  box-sizing: border-box;
  width: 25vw;
  height: 7vh;
  line-height: 7vh;
  text-align: center;
  border-radius: 10% ;
  box-shadow: black .1px .1px .5px;
}

.main {
  background-color: white;
  z-index: 1000;
  position: absolute;
  transform: perspective(2500px) rotateY(0deg);
  transition: 0.25s linear;
  min-height: 100vh;
  height: auto;
  width: 100vw;
}

.main.active {
  transform: perspective(1500px) rotateY(45deg) translateX(-20vw);
  transition: 0.25s linear;
  box-shadow: 0 0 10px black;
}

.topimage {
  position: relative;
  width: 100%;
}

.cent {
  position: relative;
  width: 100%;
}

.info_head {
  display: grid;
  grid-template-columns: 6fr 15fr;
  height: calc(100vw / 21 * 6);
  width: 100%;
  overflow: hidden;
  margin-top: -7.5vh;
}

.info_head .left_part {
  position: relative;
  width: 100%;
  height: calc(100vw / 21 * 6);
}

.info_head .right_part {
  text-overflow: clip;
  height: calc(100vw / 21 * 6);
  width: 100%;
  margin-top: 7.5vh;
}

.info_head .left_part .view_box {
  width: 90%;
  height: 90%;
  border-radius: 50%;
  border: white solid 3px;
  box-sizing: border-box;
  overflow: hidden;
  margin: 1vh 0vw 1vh 5vw;
}

.info_head .right_part .right_part_text {
  text-align: right;
  font-weight: bolder;
  margin: 0.5vh 2.5vw 1vh 4vw;
  text-shadow: 0.2px 0.2px 0.25px rgb(255, 255, 255);
  border-bottom: solid rgba(202, 202, 202, 0.75) 1px;
  padding-bottom: 1px;
  box-sizing: border-box;
  font-style: oblique;
  color: rgba(141, 141, 141, 0.75);
  text-shadow: 0.5px 0.5px 0.5px rgb(255, 255, 255);
}

.info_head .right_part .right_part_tap {
  display: flex;
  flex-direction: row-reverse;
  margin: 1vh 3vw 1vh 4vw;
}

.right_part_tap .right_part_tap_item {
  border: black 1px solid;
  box-sizing: border-box;
  color: white;
  background-color: black;
  margin-left: 2vw;
  text-transform: capitalize;
  font-size: x-small;
  padding: 1.5vh 4vw 1.5vh 4vw;
  text-align: center;
  line-height: 1.5vh;
}

.right_part_tap_item.config_item {
  background-color: rgba(219, 219, 219, 0.75);
  border: rgba(219, 219, 219, 0.75) 1px solid;
  color: gray;
  padding-left: 2vw;
  padding-right: 2vw;
  opacity: 1;
  transition: 0.25s linear;
}

.right_part_tap_item.config_item.opening {
  opacity: 0;
  transition: 0.5s linear;
  -webkit-user-select:none;
}

.headIcon {
  width: 100%;
}

.mid_part {
  display: flex;
  flex-direction: column;
}

.mid_part .username {
  flex: 1;
  margin: 3vh 1vw 1vh 6vw;
  font-weight: bolder;
  font-size: 25px;
  display: flex;
}

.mid_part .username .username_timer {
  font-size: xx-small;
  margin-top: 1.65vh;
  font-weight: lighter;
  /* font-style: italic; */
  text-shadow: 0.28px 0.25px 0.5px black;
}

.mid_part .username .user_info_icon {
  width: 15px;
  height: 15px;
  margin: 1.5vh 0vw 0vh 3vw;
  box-sizing: border-box;
  background-repeat: no-repeat;
  background-size: 15px 15px;
}

.mid_part .username .username_icon {
  background-image: url("data:image/png;base64,");
}

.mid_part .username .usergender_icon_man {
  background-image: url("data:image/png;base64,");
}

.mid_part .username .usergender_icon_woman {
  background-image: url("data:image/png;base64,");
}

.mid_part .username .usergender_icon_unknow {
  background-image: url("data:image/png;base64,");
}

.mid_part .user_info {
  flex: 1;
  display: flex;
  flex-direction: row;
}

.mid_part .user_info .user_info_item {
  border: rgba(255, 255, 255, 0.5) solid 1px;
  font-size: 12px;
  box-sizing: border-box;
  margin: 0.5vh 1vw 1vh 3vw;
  padding: 1.5vw;
  background-color: rgba(212, 212, 212, 0.5);
  color: rgba(180, 179, 179, 0.60);
  font-weight: bolder;
  border-radius: 10%;
}

.mid_part .user_info .user_info_item:nth-child(1) {
  margin-left: 6vw;
}

.mid_part .user_profile {
  flex: 1;
  margin: 3vh 6vw 1vh 6vw;
  display: flex;
  flex-direction: column;
}

.mid_part .user_profile .profile_text {
  text-align: center;
  /* overflow: hidden; */
  font-style: oblique;
  font-size: small;
  font-weight: bolder;
  color: rgba(182, 182, 182, 0.75);
  text-shadow: 0.5px 0.5px 0.5px rgb(182, 182, 182);
  align-self: center;
  white-space: pre-line;
}

.user_profile .lab {
  color: black;
  width: 100%;
  font-size: xx-large;
  /* position: absolute; */
}

.lab.lab_left {
  text-align: left;
}

.lab.lab_right {
  text-align: right;
}

.followAndMoments {
  display: flex;
  flex-direction: row;
  margin: auto 3vw auto 3vw;
}

.followAndMoments .context {
  flex: 1;
  text-align: center;
  text-transform: capitalize;
  font-size: small;
  font-weight: bolder;
  text-shadow: .2px .2px .2px black;
}

.construction {
  display: flex;
  flex-direction: column;
  margin: 2vh 3vw auto 3vw;
}

.construction .swiper_opentions {
  display: flex;
  flex-direction: row;
  text-align: center;
  font-weight: bold;
  text-shadow: black 0px 0px .01px;
}

.construction .swiper_opentions .op {
  flex: 1;
  color: rgb(197, 196, 196);
  padding-bottom: 1vh;
}

.construction .swiper_opentions .op.op_active {
  color: black;
  border-bottom: solid black 2px;
  /* transition: 0.25s linear; */
}

.bar {
  width: 100%;
  height: 1px;
  border: rgb(197, 196, 196);
  box-sizing: border-box;
  background-color: rgb(197, 196, 196);
  margin-bottom: 2vh;
}

.swip {
  background-color: rgb(90, 211, 248);
  color: white;
  width: 100%;
  /* min-height: 50vh; */
}
.swip .vi_item {
  width: 100vw;
  display: flex;
  flex-direction: column;
}